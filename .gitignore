# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# OSX IGNORE FILES
**/.DS_Store
.DS_Store
*.swp
*~.nib
# Thumbnails
._*

# IDEA User-specific stuff:
.idea/**/workspace.xml
.idea/**/tasks.xml
.idea/dictionaries

# HIDE SECRETS YML
/config/secrets.yml


/node_modules
/yarn-error.log

.byebug_history
